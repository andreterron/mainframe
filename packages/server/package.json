{
  "name": "@mainframe-api/server",
  "version": "0.8.11",
  "private": false,
  "description": "",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsup --format esm --watch src",
    "dev-prep": "tsup --format esm",
    "build": "tsup --clean",
    "publish-package": "npm publish",
    "publish-jsr": "jsr publish --allow-slow-types",
    "connect:generate": "drizzle-kit generate --config=connect-drizzle.config.ts",
    "connect:drop": "drizzle-kit drop --config=connect-drizzle.config.ts",
    "connect:push": "drizzle-kit push --config=connect-drizzle.config.ts",
    "connect:migrate": "drizzle-kit migrate --config=connect-drizzle.config.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@libsql/client": "^0.14.0",
    "@types/cookie": "^0.6.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/lodash-es": "^4.17.12",
    "jsr": "^0.12.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.3.2",
    "@hono/zod-validator": "^0.2.2",
    "@mainframe-api/shared": "0.8.11",
    "@nangohq/node": "^0.42.4",
    "@nangohq/types": "^0.42.4",
    "@sentry/core": "^8.15.0",
    "@trpc/server": "^10.45.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "cookie-signature": "^1.2.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.31.2",
    "envsafe": "^2.0.3",
    "googleapis": "^128.0.0",
    "hono": "^4.4.3",
    "json-to-ts": "^2.1.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "openai": "^4.52.3",
    "typed-emitter": "^2.1.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "@libsql/client": "*"
  },
  "peerDependenciesMeta": {
    "@libsql/client": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.21.5",
    "@libsql/win32-x64-msvc": "^0.4.7",
    "@rollup/rollup-linux-x64-gnu": "^4.18.0",
    "@rollup/rollup-win32-x64-msvc": "^4.18.0"
  }
